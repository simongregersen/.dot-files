[user]
        name = Simon Gregersen
        email = simonsog@gmail.com
[push]
        default = simple
[pull]
        rebase = true
[alias]
        st = status
        ci = commit
        br = branch
        co = checkout
        slog = log --pretty=format:'%C(yellow)%h %Cred%ad %Cblue%an%Cgreen%d %Creset%s' --date=short --decorate
        hist = log --graph --abbrev-commit --decorate --format=format:'%C(bold blue)%h%C(reset) - %C(bold cyan)%aD%C(reset) %C(bold green)(%ar)%C(reset)%C(bold yellow)%d%C(reset)%n''          %C(white)%s%C(reset) %C(dim white)- %an%C(reset)' -10
[core]
        editor = emacs
[credential]
        helper = cache --timeout=3600
